{
	"name": "rss-aggregator",
	"version": "0.8.0",
	"description": "Start reading RSS today! It's easy, it's beautiful",
	"private": true,
	"browserslist": "> 0.25%, not dead",
	"scripts": {
		"prepare": "husky install",
		"test": "npx eslint .",
		"dev": "cross-env NODE_ENV=development npx webpack --mode development",
		"build": "cross-env NODE_ENV=production npx webpack --mode production",
		"start": "cross-env NODE_ENV=development webpack serve --mode development --open"
	},
